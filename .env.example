# Analytics & Monitoring
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Development
VITE_DEBUG_MODE=true
VITE_MOCK_API=false
VITE_SHOW_DEV_TOOLS=true

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx,xls,xlsx

# Localization
VITE_DEFAULT_LANGUAGE=tr
VITE_SUPPORTED_LANGUAGES=tr,en,ar

# Map Services
VITE_MAP_API_KEY=your_map_api_key_here
VITE_DEFAULT_MAP_CENTER_LAT=39.9334
VITE_DEFAULT_MAP_CENTER_LNG=32.8597

# Performance
VITE_ENABLE_CACHING=true
VITE_CACHE_TTL=300000
